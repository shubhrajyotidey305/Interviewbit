bool isPrime(int n){
    if (n <= 1) return false;
    for (int i = 2; i <= sqrt(n); i++)
        if (n % i == 0) return false;
    return true;
}

vector<int> Solution::primesum(int A) {
    vector<int> ans(2);
   
   
   for(int i=2; i<=A; i++){
       
       if(isPrime(i) && isPrime(A-i)){
           ans[0] = i;
           ans[1] = A-i;
           break;
       }
   }

   return ans;
    
}
